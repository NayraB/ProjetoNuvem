<Window x:Class="Teste.View.Excluir"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Teste.View"
        mc:Ignorable="d"
        Title="Listar" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Image/tela_principal.jpg"/>
        </Grid.Background>
        <Button x:Name="btnVoltar" Content="Voltar" HorizontalAlignment="Left" Margin="59,177,0,0" VerticalAlignment="Top" Width="75" Click="btnVoltar_Click" />
        <Button x:Name="btnSair" Content="Sair" HorizontalAlignment="Left" Margin="59,212,0,0" VerticalAlignment="Top" Width="75" Click="btnSair_Click"/>
        <Button x:Name="btnExcluirCliente" Click="btnExcluirCliente_Click" Content="Cliente" HorizontalAlignment="Left" Margin="42,55,0,0" VerticalAlignment="Top" Width="105" Height="24"/>
        <Button x:Name="btnExcluirVeiculo" Click="btnExcluirVeiculo_Click" Content="Veiculo" HorizontalAlignment="Left" Margin="42,96,0,0" VerticalAlignment="Top" Width="105" Height="25"/>
        <Label Content="Selecione uma opção:" HorizontalAlignment="Left" Margin="30,24,0,0" VerticalAlignment="Top"/>

    </Grid>

</Window>
